
import com.github.leokash.adventofcode.utils.*

private const val PART_ONE_EXPECTED = 0
private const val PART_TWO_EXPECTED = 0

fun main() {
    Logger.enabled = true
    fun compute(input: List<String>, partOne: Boolean = true): Int = TODO()

    fun part1(input: List<String>): Int = compute(input)
    fun part2(input: List<String>): Int = compute(input, false)

    val input = readLines("Day{{DAY}}")
    val inputTest = readLines("Day{{DAY}}-Test")

    check(part1(inputTest) == PART_ONE_EXPECTED)
    println(part1(input))

    check(part2(inputTest) == PART_TWO_EXPECTED)
    println(part2(input))
}
